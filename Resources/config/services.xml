<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="melk_mjr_box_select.contributions.css_resources_provider"
                 class="Melk\MJRBoxSelectBundle\Contributions\CssResourcesProvider">
            <tag name="modera_mjr_integration.css_resources_provider" />
        </service>

        <service id="melk_mjr_box_select.contributions.js_resources_provider"
                 class="Melk\MJRBoxSelectBundle\Contributions\JsResourcesProvider">
            <tag name="modera_mjr_integration.js_resources_provider" />
        </service>

        <service id="melk_mjr_box_select.contributions.steroid_class_mappings_provider"
                 class="Melk\MJRBoxSelectBundle\Contributions\SteroidClassMappingsProvider">

            <tag name="modera_backend_on_steroids.extjs_classes_paths_provider" />
        </service>
    </services>

</container>
